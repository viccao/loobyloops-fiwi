{
  "rules": {
    "strict": [2, "global"],
    "quotes": [2, "single"],
    "semi": 2,
    "curly": 2,
    "valid-typeof": 2,
    "comma-dangle": [2, "never"],
    "eqeqeq": 2,
    "default-case": 2,
    "dot-notation": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "array-bracket-spacing": [2, "never"],
    "block-spacing": [2, "always"],
    "brace-style": [2, "stroustrup", { "allowSingleLine": false }],
    "camelcase": [2, {"properties": "never"}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "consistent-this": [2, "_this"],
    "func-names": 2,
    "func-style": [2, "expression"],
    "indent": [2, 2],
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 2,
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-infix-ops": 2,
    
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-spaced-func": 2,
    "no-underscore-dangle": 2,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-sparse-arrays": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-negated-in-lhs": 2,
    "no-irregular-whitespace": 2,
    "no-invalid-regexp": 2,
    "no-inner-declarations": [2, "functions"],
    "no-func-assign": 2,
    "no-extra-semi": 2,
    "no-extra-parens": 2,
    "no-extra-boolean-cast": 2,
    "no-ex-assign": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-duplicate-case": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-floating-decimal": 2,
    "no-implied-eval" : 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new-wrappers": 2,
    "no-param-reassign": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-with": 2,
    "no-undef-init": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
  }
}
